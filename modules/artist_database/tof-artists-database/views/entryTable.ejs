<% if(role === "worker") { %>
    <% include headerWorker %>
    <% } %> 
 
    <% if(role === "admin") { %>
     <% include headerAdmin %>
     <% } %> 
     
 

    <div class="breadcrumbs">
        <div class="col-sm-4">
            <div class="page-header float-left">
                <div class="page-title">
                    <h1>Entries List</h1>
                </div>
            </div>
        </div>
        <div class="col-sm-8">
            <div class="page-header float-right">
                <div class="page-title">
                    <ol class="breadcrumb text-right">
                        <!-- <li><a href="#">Dashboard</a></li>
                        <li><a href="#">Table</a></li>
                        <li class="active">Data table</li> -->
                    </ol>
                </div>
            </div>
        </div>
    </div>

    <div class="content mt-3">
        <div class="animated fadeIn">
            <div class="row">

                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">Data</strong>
                        </div>
                        <div class="card-body">
                            <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                                <thead>
                                       
                                    <tr>
                                        <th>First Name</th>
                                        <th>Middle Name</th>
                                        <th>Last Name</th>
                                        <th>Email</th>
                                        <th>Action</th>

                                    </tr>
                                </thead>
                                <tbody>
                                        <% for(var i=0; i < data.length; i++){ %>
                                    <tr>
                                
                                        <td><%= data[i].firstName %></td>
                                        <td><%= data[i].middletName %></td>
                                        <td><%= data[i].lastName %></td>
                                        <td><%= data[i].email %></td>
                                        <td>
                                            <a href="viewartist?id=<%= data[i]._id %>"><i class="fa fa-eye"></i></a>
                                            <a href="editartist?id=<%= data[i]._id %>"><i class="fa fa-edit"></i></a>
                                            <% if(role === "admin"){ %>
                                            <a href="delete?id=<%= data[i]._id %>"><i class="fa fa-remove"></i></a>
                                            <% } %>
                                        </td>
                                    </tr>
                                    <% } %>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>


            </div>
        </div>
        <!-- .animated -->
    </div>
    <!-- .content -->


    </div>
    <!-- /#right-panel -->

    <!-- Right Panel -->

   












